{
  "name": "static-site-creator",
  "version": "1.0.2",
  "description": "Creator of static sites content for template managers (like pug ex-blade)",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "build": "gulp build",
    "build:production": "gulp build --production",
    "postinstall": "npm run build:production",
    "start": "node server.js",
    "start-heroku": "node heroku.js && node server.js",
    "start:production": "pm2 start server.js -i 4",
    "test": "npm run test:server && npm run test:client",
    "test:client": "karma start app/karma.conf.js --single-run",
    "test:server": "mocha --recursive",
    "watch": "gulp"
  },
  "dependencies": {
    "angular-base64-upload": "^0.1.19",
    "async": "^1.5.2",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.15.1",
    "bookshelf": "^0.9.2",
    "brfs": "^1.4.3",
    "browserify": "^13.1.0",
    "checklist-model": "^0.10.0",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.1",
    "cron": "^1.1.0",
    "crypto": "0.0.3",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "express-validator": "^2.20.4",
    "gulp": "^3.9.1",
    "gulp-angular-templatecache": "latest",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "latest",
    "gulp-csso": "^1.1.0",
    "gulp-if": "^2.0.0",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "jsonwebtoken": "^5.7.0",
    "knex": "^0.10.0",
    "md5": "^2.1.0",
    "moment": "^2.12.0",
    "morgan": "^1.7.0",
    "nodegit": "^0.27.0",
    "nodemailer": "^2.3.0",
    "pg": "^6.0.1",
    "pm2": "^1.0.2",
    "pug": "^2.0.0-beta4",
    "request": "^2.74.0",
    "request-promise": "^4.0.2",
    "sanitize-filename": "^1.6.0",
    "sass": "^1.56.1",
